#include<stdio.h>
int main(){
    int flag=0;
    int n;
    printf("enter the number=");
    scanf("%d",&n);
    for(int i=2;i<=n/2;i++){
        if(n%i==0){
            flag=1;
            break;
        }
    }
    if(flag==0) printf("it is not a prime number=%0d",n);
    else printf("it is a prime number=%0d",n);
    
}
