
#include <stdio.h>

int main(){
int array[6]={1,2,3,4,5,6};
for(int i=0;i<6;i++){
    printf("%p\n",&array[i]);
}
printf("\n%p",&array[0]);
printf("\n%p",array);
printf("\n%d",*array);
    printf("\n%d",*array+1);
    printf("\n%d",*array+5);
    printf("\n%d",*array+4);
    printf("\n%d",*array+3);
    
}
